<?xml version="1.0"?>
<ArrayOfTargetDeviceFamily xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<!-- ============================================ STM32 ============================================ -->
	<TargetDeviceFamily>
		<ID>STM32F0</ID>
		<Name>STM32F0xxx</Name>
		<MatchRegex>^STM32F0.*</MatchRegex>
		<BspID>com.sysprogs.arm.stm32</BspID>
		<ScriptFile>target/stm32f0x.cfg</ScriptFile>
	</TargetDeviceFamily>
	<TargetDeviceFamily>
		<ID>STM32F1</ID>
		<Name>STM32F1xxx</Name>
		<MatchRegex>^STM32F1.*</MatchRegex>
		<BspID>com.sysprogs.arm.stm32</BspID>
		<ScriptFile>target/stm32f1x.cfg</ScriptFile>
		<SpecifiesAdapterSpeed>true</SpecifiesAdapterSpeed>
	</TargetDeviceFamily>
